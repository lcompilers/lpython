    assert (__lpython_overloaded_0__abs((float)(__lpython_overloaded_0__pow(6, -4)) - 7.716049e-04) < eps);
    assert (__lpython_overloaded_0__abs(__lpython_overloaded_3__pow(4.500000e+00, 2.300000e+00) - 3.179719e+01) < eps);
    assert (__lpython_overloaded_0__abs(__lpython_overloaded_3__pow(2.300000e+00, 0.000000e+00) - 1.000000e+00) < eps);
    assert (__lpython_overloaded_0__abs(__lpython_overloaded_3__pow(2.300000e+00, -1.500000e+00) - 2.866872e-01) < eps);
